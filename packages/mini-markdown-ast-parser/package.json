{
  "name": "@mini-markdown-resin/ast-parser",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./style": {
      "import": "./dist/style/index.css",
      "require": "./dist/style/index.css"
    },
    "./*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "rollup -c rollup.config.ts --configPlugin typescript -w",
    "start": "nodemon dist/index.js",
    "dev": "npm-run-all --parallel watch start",
    "build": "pnpm lint && rollup -c rollup.config.ts --configPlugin typescript",
    "lint": "eslint .",
    "build:play": "pnpm build && node example/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.17.1",
  "dependencies": {
    "highlight.js": "^11.11.1"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^22.18.6",
    "@typescript-eslint/parser": "8.20.0",
    "eslint": "9.18.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "rollup": "^4.52.3",
    "rollup-plugin-delete": "^2.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  }
}
